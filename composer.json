{
    "name": "adsy2010/laravel-companies-house",
    "description": "This the Laravel Service Provider package to access CompaniesHouse API within your Laravel application.",
    "keywords": ["companies-house", "laravel"],
    "type": "library",
    "license": "MIT",
    "version": "0.10.0",
    "authors": [
        {
            "name": "Adam Wright",
            "email": "adsy2010@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Adsy2010\\CompaniesHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adsy2010\\CompaniesHouse\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Adsy2010\\CompaniesHouse\\CompaniesHouseServiceProvider"
            ],
            "aliases": {
                "CompaniesHouse": "Adsy2010\\CompaniesHouse\\Facades\\CompaniesHouse"
            }
        }
    }
}
